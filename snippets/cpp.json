    "ZValues.h": {
        "body": [
            "vector<int> zValues(string& s) {",
            "\tint n = ( int )s.length();",
            "\tvector<int> z(n);",
            "\tfor (int i = 1, l = 0, r = 0; i < n; ++i) {",
            "\t\tif (i <= r) z[i] = min(r - i + 1, z[i - l]);",
            "\t\twhile (i + z[i] < n && s[z[i]] == s[i + z[i]]) ++z[i];",
            "\t\tif (i + z[i] - 1 > r) l = i, r = i + z[i] - 1;",
            "\t}",
            "\treturn z;",
            "}"
        ],
        "prefix": [
            "hack_ZValues"
        ]
    },
